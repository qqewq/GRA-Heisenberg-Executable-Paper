   "cell_type": "code",
   "source": [
    "# Эрдёш #728: Факториалы в модулярной арифметике\n",
    "# Цель: доказать существование n! ≡ 1 (mod m) для всех m\n",
    "import sys\n",
    "sys.path.append('../src')\n",
    "from gra_core import GRAMetaNulling, DomainGoal\n",
    "from hilbert_space import HilbertSpace\n",
    "import torch\n",
    "\n",
    "# Гильбертово пространство размерности 100 (факториалы до 100)\n",
    "H_factorial = HilbertSpace(100)\n",
    "\n",
    "# Проектор цели: n! → 1 mod n для всех n\n",
    "goal_states = torch.eye(100, dtype=torch.complex64)\n",
    "P_728 = H_factorial.project_to_goal(goal_states)\n",
    "\n",
    "# Создание домена\n",
    "domain_728 = DomainGoal('Erdos_728', P_728, 100)\n",
    "\n",
    "# Запуск мета-обнулёнки\n",
    "gra = GRAMetaNulling([domain_728], lambda_meta=1.0)\n",
    "history = gra.optimize(epochs=500)\n",
    "\n",
    "print(f'Φ^(728) = {history[\"local_foams\"][-1][0]:.2e}')\n",
    "print('✅ Обнуление достигнуто!' if history[\"local_foams\"][-1][0] < 1e-6 else '❌')"
   ],
   "outputs": [],
   "execution_count": null
  }
